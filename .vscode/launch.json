  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "GrpcClient",
      "request": "launch",
      "mainClass": "com.GrpcClient"
    },
    {
      "type": "java",
      "name": "GrpcClient",
      "request": "launch",
      "mainClass": "com.GrpcClient",
      "projectName": "grpcdemoclient"
    },
    {
      "type": "java",
      "name": "GrpcServer",
